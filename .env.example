# Discord
DISCORD_TOKEN=
DISCORD_CLIENT_ID=
DISCORD_GUILD_ID=
ACTIVE_CHANNEL_ID=
OWNER_DISCORD_ID=

# Roblox
# Main group ID
MAIN_GROUP_ID=

# Roblox Open Cloud authentication
# Option A (recommended): User API key (Dashboard -> Open Cloud -> API Keys)
ROBLOX_API_KEY=
# Option B: OAuth token (set either ROBLOX_OAUTH_TOKEN, or put it into ROBLOX_API_KEY as: Bearer <token>)
ROBLOX_OAUTH_TOKEN=

# RoWiFi (optional but recommended for secure /giris without asking username)
ROWIFI_TOKEN=
# API version (see https://rowifi.xyz/docs/api). Default: 3
ROWIFI_API_VERSION=3

# Optional: cookie used for /bransdan-at legacy endpoint (groups.roblox.com) if Open Cloud doesn't support removal
# WARNING: Keep private and never commit.
ROBLOX_COOKIE=
